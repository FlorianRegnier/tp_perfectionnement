{% extends "admin/base.html.twig" %}


{% block main %}

    {% for message in app.flashes('notice') %}
        <div class="flash-notice">
            {{ message }}
        </div>
    {% endfor %}

    <h1>Liste des images</h1>

    {% for media in medias %}
       
        <a href="{{ path('admin_show_media', { 'id' : media.id }) }}" >
            <h2  > {{ media.title }} </h2>
        </a>
        <h3> {{ media.category.name }} </h3>
                      
    {% endfor %}



    
{% endblock %}